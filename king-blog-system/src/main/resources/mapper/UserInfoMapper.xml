<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.king.sys.mapper.UserInfoMapper">

    <select id="findGenderByToken" resultType="java.lang.Integer">
        select `gender` from `t_user_info` where `token` = #{token};
    </select>

</mapper>
